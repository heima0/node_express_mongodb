      <style type="text/css">
        body{
          padding-bottom: 0;
        }
        .carousel-inner>.item.active, .carousel-inner>.item.next.left, .carousel-inner>.item.prev.right{
            display: flex;
            align-items: center;
        }
        .carousel-inner{
          height: 500px;
        }
        .carousel-inner>.item{
          height: 500px;
        }

        .widget {
            margin-bottom: 35px
        }

        .widget .title {
            margin-top: 0;
            padding-bottom: 7px;
            border-bottom: 1px solid #ebebeb;
            margin-bottom: 21px;
            position: relative
        }

        .widget .title:after {
            content: "";
            width: 90px;
            height: 1px;
            background: #337ab7;
            position: absolute;
            left: 0;
            bottom: -1px
        }

        .widget .friend-links a {
            display: block;
            border-bottom: 1px dashed #303030;
            padding-bottom: 14px;
            margin-bottom: 14px;
            color: #959595;
            -webkit-transition: all .2s ease;
            -o-transition: all .2s ease;
            transition: all .2s ease
        }

        .widget .friend-links a:hover {
            color: #fff;
            text-decoration: none
        }

        .widget .friend-links a:last-child {
            margin-bottom: 0
        }

        .widget .tag-cloud a {
            border: 1px solid #ebebeb;
            padding: 2px 7px;
            color: #959595;
            line-height: 1.5em;
            display: inline-block;
            margin: 0 7px 7px 0;
            -webkit-transition: all .2s ease;
            -o-transition: all .2s ease;
            transition: all .2s ease
        }

        .widget .tag-cloud a:hover {
            color: #ffffff;
            background: #337ab7;
            border: 1px solid #337ab7;
            text-decoration: none
        }

        .submit-site {
            background-color: #eee;
            padding-top: 40px;
            padding-bottom: 40px;
            margin-top: 20px
        }


        .main-footer {
            background: #202020;
            padding: 35px 0 0;
            color: #959595
        }

        .main-footer .widget {
            padding: 0 30px
        }

        .main-footer .widget .title {
            color: #ffffff;
            border-bottom: 1px solid #303030
        }

        .main-footer .widget .tag-cloud a {
            border: 1px solid #303030
        }

        .main-footer .widget .tag-cloud a:hover {
            border: 1px solid #337ab7
        }

      </style>
